def main
        key = 0x80
        enc = [211, 203, 217, 173, 195, 193, 212, 173, 180, 181, 182, 185]

        puts "Can I haz auth?"
        auth = gets.chomp.bytes.to_a
        if enc ==  auth.map! {|a| key ^ a} then
                puts "Congrats"
        else
                puts "fail"
        end
end

main
